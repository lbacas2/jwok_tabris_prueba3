<?xml version='1.0' encoding='utf-8'?>
<widget id="icariaCRM.app" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>icariaCRM</name>
	<description>
		icaria CRM. Versión móvil.
	</description>
	<author email="luis.bacas.riveiro@netzima.com" href="http://www.netzima.com">
		Netzima team
	</author>
	<content src="package.json"/>
	<plugin name="cordova-plugin-dialogs" version="1.1.1" />
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
</widget>